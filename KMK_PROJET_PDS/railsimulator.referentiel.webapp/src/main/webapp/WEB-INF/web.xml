<?xml version="1.0" encoding="UTF-8"?>
<web-app>
	<servlet>
		<servlet-name>AccueilControler</servlet-name>
		<servlet-class>servlets.AccueilControler</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>LigneControler</servlet-name>
		<servlet-class>servlets.LigneControler</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>CantonControler</servlet-name>
		<servlet-class>servlets.CantonControler</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>StationControler</servlet-name>
		<servlet-class>servlets.StationControler</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>ReseauControler</servlet-name>
		<servlet-class>servlets.ReseauControler</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>HoraireControler</servlet-name>
		<servlet-class>servlets.HoraireControler</servlet-class>
	</servlet>


	<servlet-mapping>
		<servlet-name>LigneControler</servlet-name>
		<url-pattern>/ligne</url-pattern>
	</servlet-mapping>


	<servlet-mapping>
		<servlet-name>AccueilControler</servlet-name>
		<url-pattern>/accueil</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>CantonControler</servlet-name>
		<url-pattern>/canton</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>StationControler</servlet-name>
		<url-pattern>/station</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>ReseauControler</servlet-name>
		<url-pattern>/reseau</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>HoraireControler</servlet-name>
		<url-pattern>/horaire</url-pattern>
	</servlet-mapping>
</web-app>