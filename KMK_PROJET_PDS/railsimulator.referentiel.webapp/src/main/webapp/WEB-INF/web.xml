	</servlet>

	<servlet>
		<servlet-name>WorkflowControler</servlet-name>
		<servlet-class>railsimulator.workflow.WorkflowControler</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>OptimisationControler</servlet-name>
